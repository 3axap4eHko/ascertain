{
  "name": "ascertain",
  "version": "0.14.18",
  "description": "0-Deps, simple, fast, for browser and node js object schema validator",
  "type": "module",
  "types": "build/index.d.ts",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "exports": {
    "require": "./build/index.cjs",
    "import": "./build/index.js"
  },
  "files": [
    "build",
    "src/index.js"
  ],
  "scripts": {
    "build": "rm -rf build && NODE_ENV=production inop src/ build -t js -i __tests__ && cp src/index.d.ts build",
    "build:2": "NODE_ENV=production swc src/index.js -d build --ignore **/__tests__,**/__fixtures__ --strip-leading-paths && cp src/index.d.ts build",
    "test": "NODE_ENV=test jest",
    "test:cov": "COVERAGE=1 NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/ascertain.git"
  },
  "keywords": [
    "javascript",
    "js",
    "schema",
    "validation",
    "js-validation",
    "validator",
    "js-validator",
    "assert",
    "ascertain",
    "browser",
    "node"
  ],
  "author": "Ivan Zakharchanka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3axap4eHko/ascertain/issues"
  },
  "homepage": "https://github.com/3axap4eHko/ascertain#readme",
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "inop": "^0.6.11",
    "jest": "^29.7.0"
  }
}
